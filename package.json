{
  "name": "nodejs-seed",
  "version": "0.1.0",
  "description": "Node.js seed project for Babel and ESLint building to a single directory of one code file and separate resource files",
  "scripts": {
    "start": "cd dist && node app.js",
    "serve": "node_modules/.bin/nodemon --watch src/ --exec 'npm run build && npm start'",
    "build": "node_modules/.bin/webpack",
    "test": "test/unit/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/talee/nodejs-seed.git"
  },
  "keywords": [
    "node",
    "seed",
    "starter",
    "project"
  ],
  "author": "Thomas Lee <tleehoc@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/talee/nodejs-seed/issues"
  },
  "homepage": "https://github.com/talee/nodejs-seed#readme",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "file-loader": "^0.10.0",
    "nodemon": "^1.11.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "source-map-support": "^0.4.11"
  }
}
